simulate_dice <- function(N) {
relativ_frequencies <- c(0, 0, 0, 0, 0, 0)
for (i in c(1:N)){
x <- sample((1:6), 1)
print(x)
relativ_frequencies[x] <- relativ_frequencies[x] + 1
}
relativ_frequencies <- relativ_frequencies / N
return(relativ_frequencies)
}
